{
  "name": "@code-snippet/requests",
  "version": "0.2.0",
  "type": "module",
  "description": "基于 Axios 封装的 TypeScript HTTP 请求库，支持自定义响应格式",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "bun x tsc --noEmit"
  },
  "peerDependencies": {
    "axios": "^1.0.0"
  },
  "peerDependenciesMeta": {
    "axios": {
      "optional": false
    }
  },
  "devDependencies": {
    "axios": "^1.9.0",
    "typescript": "^5.7.3"
  },
  "keywords": [
    "axios",
    "http",
    "request",
    "typescript",
    "api-client"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/user/code-snippet.git",
    "directory": "packages/requests"
  }
}
